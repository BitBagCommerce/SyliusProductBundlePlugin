        "productVariant": {
          "@id": "/api/v2/shop/product-variants/JACK_DANIELS_GENTLEMAN",
          "@type": "ProductVariant",
          "code": "JACK_DANIELS_GENTLEMAN",
          "optionValues": "@*@",
          "translations": {
            "en_US": {
              "@id": "/api/v2/shop/product-variant-translation/@integer@",
              "@type": "ProductVariantTranslation",
              "name": "Jack Daniel's Gentleman Jack"
            }
          },
          "price": "@integer@",
          "inStock": "@boolean@"
        }
      }
    ]
  },
